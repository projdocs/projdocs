<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0"
           xsi:type="TaskPaneApp">
  <Id>02BAD963-836A-4517-9CF2-300D4A81FF5C</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>Train360, Corp.</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="ProjDocs"/>
  <Description DefaultValue="ProjDocs Connector"/>
  <IconUrl DefaultValue="https://projdocs.com/icons/icon-32.png"/>
  <HighResolutionIconUrl DefaultValue="https://projdocs.com/icons/64-icon.png"/>
  <SupportUrl DefaultValue="https://projdocs.com"/>

  <AppDomains>
    <AppDomain>https://projdocs.com</AppDomain>
  </AppDomains>

  <Hosts>
    <Host Name="Document"/>
  </Hosts>

  <Requirements>
    <Sets DefaultMinVersion="1.1">
      <Set Name="SharedRuntime" MinVersion="1.1"/>
    </Sets>
  </Requirements>

  <DefaultSettings>
    <SourceLocation DefaultValue="https://projdocs.com/src/surfaces/ribbon/index.html"/>
  </DefaultSettings>

  <Permissions>ReadWriteDocument</Permissions>

  <VersionOverrides
      xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides"
      xsi:type="VersionOverridesV1_0"
  >

    <Hosts>
      <Host xsi:type="Document">

        <Runtimes>
          <Runtime resid="Commands.Url" lifetime="long"/>
        </Runtimes>

        <DesktopFormFactor>
          <GetStarted>
            <Title resid="GetStarted.Title"/>
            <Description resid="GetStarted.Description"/>
            <LearnMoreUrl resid="GetStarted.LearnMoreUrl"/>
          </GetStarted>
          <FunctionFile resid="Commands.Url"/>
          <ExtensionPoint xsi:type="PrimaryCommandSurface">

            <CustomTab id="PD.CT">

<!--              <Group id="PD.CT.GA">-->
<!--                <Label resid="PD.CT.GA.Label"/>-->
<!--                <Icon>-->
<!--                  <bt:Image size="16" resid="Icon.16x16"/>-->
<!--                  <bt:Image size="32" resid="Icon.32x32"/>-->
<!--                  <bt:Image size="80" resid="Icon.80x80"/>-->
<!--                </Icon>-->
<!--                <Control xsi:type="Button" id="LaunchProjDocsButton">-->
<!--                  <Label resid="LaunchProjDocsButton.Label"/>-->
<!--                  <Supertip>-->
<!--                    <Title resid="LaunchProjDocsButton.Label"/>-->
<!--                    <Description resid="LaunchProjDocsButton.Tooltip"/>-->
<!--                  </Supertip>-->
<!--                  <Icon>-->
<!--                    <bt:Image size="16" resid="Icon.16x16"/>-->
<!--                    <bt:Image size="32" resid="Icon.32x32"/>-->
<!--                    <bt:Image size="80" resid="Icon.80x80"/>-->
<!--                  </Icon>-->
<!--                  <Action xsi:type="ExecuteFunction">-->
<!--                    <FunctionName>launch</FunctionName>-->
<!--                  </Action>-->
<!--                </Control>-->
<!--              </Group>-->

              <!-- MAIN COMMAND GROUP -->
              <Group id="PD.CT.GB">
                <Label resid="PD.CT.GB.Label"/>
                <Icon>
                  <bt:Image size="16" resid="Icon.16x16"/>
                  <bt:Image size="32" resid="Icon.32x32"/>
                  <bt:Image size="80" resid="Icon.80x80"/>
                </Icon>

                <!-- Button 1: Save -->
                <Control xsi:type="Button" id="SaveButton">
                  <Label resid="SaveButton.Label"/>
                  <Supertip>
                    <Title resid="SaveButton.Label"/>
                    <Description resid="SaveButton.Tooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.Save.16x16"/>
                    <bt:Image size="32" resid="Icon.Save.32x32"/>
                    <bt:Image size="80" resid="Icon.Save.80x80"/>
                  </Icon>
                  <Action xsi:type="ExecuteFunction">
                    <FunctionName>save</FunctionName>
                  </Action>
                  <Enabled>false</Enabled>
                </Control>

                <!-- Button 2: Save As New Version -->
                <Control xsi:type="Button" id="SaveAsNewVersionButton">
                  <Label resid="SaveAsNewVersionButton.Label"/>
                  <Supertip>
                    <Title resid="SaveAsNewVersionButton.Label"/>
                    <Description resid="SaveAsNewVersionButton.Tooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.SaveAsNewVersion.16x16"/>
                    <bt:Image size="32" resid="Icon.SaveAsNewVersion.32x32"/>
                    <bt:Image size="80" resid="Icon.SaveAsNewVersion.80x80"/>
                  </Icon>
                  <Action xsi:type="ExecuteFunction">
                    <FunctionName>saveAsNewVersion</FunctionName>
                  </Action>
                  <Enabled>false</Enabled>
                </Control>

                <!-- Button 3: Save As New Document -->
                <Control xsi:type="Button" id="SaveAsNewDocumentButton">
                  <Label resid="SaveAsNewDocumentButton.Label"/>
                  <Supertip>
                    <Title resid="SaveAsNewDocumentButton.Label"/>
                    <Description resid="SaveAsNewDocumentButton.Tooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.SaveAsNewDocument.16x16"/>
                    <bt:Image size="32" resid="Icon.SaveAsNewDocument.32x32"/>
                    <bt:Image size="80" resid="Icon.SaveAsNewDocument.80x80"/>
                  </Icon>
                  <Action xsi:type="ExecuteFunction">
                    <FunctionName>saveAsNewDoc</FunctionName>
                  </Action>
                  <Enabled>true</Enabled>
                </Control>
              </Group>

              <Group id="PD.CT.GC">
                <Label resid="PD.CT.GC.L"/>
                <Icon>
                  <bt:Image size="16" resid="Icon.16x16"/>
                  <bt:Image size="32" resid="Icon.32x32"/>
                  <bt:Image size="80" resid="Icon.80x80"/>
                </Icon>
                <Control xsi:type="Button" id="InsertDocID">
                  <Label resid="PD.CT.GC.M.L"/>
                  <Supertip>
                    <Title resid="PD.CT.GC.M.L"/>
                    <Description resid="PD.CT.GC.M.T"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.InsertDocID.16"/>
                    <bt:Image size="32" resid="Icon.InsertDocID.32"/>
                    <bt:Image size="80" resid="Icon.InsertDocID.80"/>
                  </Icon>
                  <Action xsi:type="ExecuteFunction">
                    <FunctionName>insertDocID</FunctionName>
                  </Action>
                  <Enabled>false</Enabled>
                </Control>
              </Group>

              <Label resid="PD.CT.Label"/>
            </CustomTab>
          </ExtensionPoint>
        </DesktopFormFactor>
      </Host>
    </Hosts>
    <Resources>
      <bt:Images>
        <bt:Image id="Icon.16x16" DefaultValue="https://projdocs.com/icons/16-icon.png"/>
        <bt:Image id="Icon.32x32" DefaultValue="https://projdocs.com/icons/32-icon.png"/>
        <bt:Image id="Icon.80x80" DefaultValue="https://projdocs.com/icons/80-icon.png"/>

        <!-- Button 1: Save -->
        <bt:Image id="Icon.Save.16x16" DefaultValue="https://projdocs.com/icons/save-16.png"/>
        <bt:Image id="Icon.Save.32x32" DefaultValue="https://projdocs.com/icons/save-32.png"/>
        <bt:Image id="Icon.Save.80x80" DefaultValue="https://projdocs.com/icons/save-80.png"/>

        <!-- Button 2: Save As New Version -->
        <bt:Image id="Icon.SaveAsNewVersion.16x16" DefaultValue="https://projdocs.com/icons/new-version-16.png"/>
        <bt:Image id="Icon.SaveAsNewVersion.32x32" DefaultValue="https://projdocs.com/icons/new-version-32.png"/>
        <bt:Image id="Icon.SaveAsNewVersion.80x80" DefaultValue="https://projdocs.com/icons/new-version-80.png"/>

        <!-- Button 3: Save As New Document -->
        <bt:Image id="Icon.SaveAsNewDocument.16x16" DefaultValue="https://projdocs.com/icons/new-file-16.png"/>
        <bt:Image id="Icon.SaveAsNewDocument.32x32" DefaultValue="https://projdocs.com/icons/new-file-32.png"/>
        <bt:Image id="Icon.SaveAsNewDocument.80x80" DefaultValue="https://projdocs.com/icons/new-file-80.png"/>

        <!-- Insert Doc ID Button -->
        <bt:Image id="Icon.InsertDocID.16" DefaultValue="https://projdocs.com/icons/doc-id-16.png"/>
        <bt:Image id="Icon.InsertDocID.32" DefaultValue="https://projdocs.com/icons/doc-id-32.png"/>
        <bt:Image id="Icon.InsertDocID.80" DefaultValue="https://projdocs.com/icons/doc-id-80.png"/>

      </bt:Images>
      <bt:Urls>
        <bt:Url id="GetStarted.LearnMoreUrl" DefaultValue="https://go.microsoft.com/fwlink/?LinkId=276812"/>
        <bt:Url id="Commands.Url" DefaultValue="https://projdocs.com/src/surfaces/ribbon/index.html"/>
      </bt:Urls>
      <bt:ShortStrings>

        <bt:String id="GetStarted.Title" DefaultValue="ProjDocs loaded successfully!"/>
        <bt:String id="PD.CT.Label" DefaultValue="ProjDocs"/>

        <bt:String id="PD.CT.GA.Label" DefaultValue="Setup"/>
        <bt:String id="LaunchProjDocsButton.Label" DefaultValue="Launch ProjDocs"/>

        <bt:String id="PD.CT.GB.Label" DefaultValue="Commands Group"/>
        <bt:String id="SaveButton.Label" DefaultValue="Save Version"/>
        <bt:String id="SaveAsNewVersionButton.Label" DefaultValue="Save as New Version"/>
        <bt:String id="SaveAsNewDocumentButton.Label" DefaultValue="Save as New Document"/>

        <bt:String id="PD.CT.GC.L" DefaultValue="Tools"/>
        <bt:String id="PD.CT.GC.M.L" DefaultValue="Insert Document ID"/>

      </bt:ShortStrings>
      <bt:LongStrings>
        <bt:String
            id="GetStarted.Description"
            DefaultValue="The ProjDocs add-in loaded successfully. Go to the HOME tab to continue."
        />

        <bt:String
            id="LaunchProjDocsButton.Tooltip"
            DefaultValue="Start ProjDocs in this document and enable autoload on open."
        />

        <!-- Button 1: Save-->
        <bt:String
            id="SaveButton.Tooltip"
            DefaultValue="Save the document, overwriting the current version."
        />

        <!-- Button 2: Save As New Version -->
        <bt:String
            id="SaveAsNewVersionButton.Tooltip"
            DefaultValue="Save the document as a new version."
        />

        <!-- Button 3: Save As New Document -->
        <bt:String
            id="SaveAsNewDocumentButton.Tooltip"
            DefaultValue="Save the document as a new file."
        />

        <bt:String id="PD.CT.GC.M.T" DefaultValue="Insert the document's id at the current cursor position."/>


      </bt:LongStrings>
    </Resources>
  </VersionOverrides>
</OfficeApp>